@model OnlineShop.Models.SpecialTag
@{
    ViewData["Title"] = "Details";
}
<br />
<h2 class="text-info">Special Tag Details</h2>
<br />
<br />
<div class="p-4 rounded table-bordered">
    <br />
    <div asp-validation-summary="ModelOnly" class="text-danger">
    </div>
    <div class="form-group row">
        <div class="col-md-2">
            <label asp-for="SpecialTags"></label>
        </div>
        <div class="col-md-5">
            <input asp-for="SpecialTags" readonly="readonly" class="form-control" />
        </div>
        <span asp-validation-for="SpecialTags" class="text-danger"></span>
    </div>
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
        <a asp-action="Index" class="btn btn-success">Back to List</a>

        @*<input type="submit" class="btn btn-success" value="Back to List" />*@
    </div>
</div>
@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}